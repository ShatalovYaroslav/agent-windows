<?xml version="1.0" encoding="utf-8"?>
<agent xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <internalConfig>
    <proactiveLocation />
    <java_home />
    <jvm_parameters></jvm_parameters>    
    <enable_memory_management>false</enable_memory_management>
    <java_memory>0</java_memory>
    <native_memory>0</native_memory>
  </internalConfig>
  <events />
  <actions>
    <action xsi:type="AdvertAction">
      <initialRestartDelay>0</initialRestartDelay>
      <javaStarterClass>org.objectweb.proactive.core.util.winagent.PAAgentServiceRMIStarter</javaStarterClass>
      <isEnabled>false</isEnabled>
      <nodeName />
    </action>
    <action xsi:type="RMAction">
      <initialRestartDelay>0</initialRestartDelay>
      <javaStarterClass>org.ow2.proactive.resourcemanager.utils.PAAgentServiceRMStarter</javaStarterClass>
      <isEnabled>false</isEnabled>
      <username />
      <password />
      <url />
      <nodeName />
    </action>
    <action xsi:type="CustomAction">
      <initialRestartDelay>0</initialRestartDelay>
      <javaStarterClass>user.Starter</javaStarterClass>
      <isEnabled>false</isEnabled>
      <args></args>      
    </action>
  </actions>  
</agent>