//
// Menu
//

IDR_MAINFRAME MENU PRELOAD DISCARDABLE 
BEGIN
    POPUP "&Action"
    BEGIN
        MENUITEM "&Connect to another computer...", ID_ACTION_CONNECT
        MENUITEM "&Disconnect",                 ID_ACTION_DISCONNECT
        MENUITEM SEPARATOR
        MENUITEM "&End Process",                ID_ACTION_KILLPROC
        MENUITEM "End Process &Tree",           ID_ACTION_KILLTREE
        MENUITEM "End &Application",            ID_ACTION_KILLAPP
        MENUITEM SEPARATOR
        MENUITEM "Security...",                 ID_ACTION_SECURITY
        MENUITEM SEPARATOR
        MENUITEM "E&xit",                       ID_APP_EXIT
    END
    POPUP "&View"
    BEGIN
        MENUITEM "&Status Bar",                 ID_VIEW_STATUS_BAR
        MENUITEM SEPARATOR
        MENUITEM "&Applications",               ID_VIEW_APPLICATIONS
        MENUITEM "&Processes",                  ID_VIEW_PROCESSES
        MENUITEM SEPARATOR
        MENUITEM "&Refresh Now\tF5",            ID_VIEW_REFRESH
        POPUP "&Update Speed"
        BEGIN
            MENUITEM "&High",                       ID_VIEW_SPEED_HIGH
            MENUITEM "&Normal",                     ID_VIEW_SPEED_NORMAL
            MENUITEM "&Low",                        ID_VIEW_SPEED_LOW
            MENUITEM "&Paused",                     ID_VIEW_SPEED_PAUSED
        END
        MENUITEM SEPARATOR
        MENUITEM "Show 1&6-bit Tasks",          ID_VIEW_16BIT
    END
    POPUP "&Options"
    BEGIN
        POPUP "Enumerate &Processes With"
        BEGIN
            MENUITEM "&1 PSAPI",                    ID_OPTIONS_ENUMPROC_PSAPI

            MENUITEM "&2 ToolHelp32 API",           ID_OPTIONS_ENUMPROC_TOOLHELP

            MENUITEM "&3 ZwQuerySystemInformation", ID_OPTIONS_ENUMPROC_NTAPI

            MENUITEM "&4 Performance Data",         ID_OPTIONS_ENUMPROC_PERFDATA

            MENUITEM "&5 Windows Management Instrumentation", 
                                                    ID_OPTIONS_ENUMPROC_WMI
        END
        POPUP "&Detect Hangup With"
        BEGIN
            MENUITEM "&1 SendMessageTimeout",       ID_OPTIONS_HANGUP_SMTO
            MENUITEM "&2 IsHungAppWindow/IsHungThread", 
                                                    ID_OPTIONS_HANGUP_UNDOC
        END
        MENUITEM SEPARATOR
        MENUITEM "Enable &Debug Privilege",     ID_OPTIONS_DEBUG
    END
END

IDR_POPUPS MENU DISCARDABLE 
BEGIN
